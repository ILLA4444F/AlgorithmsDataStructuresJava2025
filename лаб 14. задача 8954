import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();           // кількість елементів масиву
        int[] arr = new int[n];         // створюємо масив

        for (int i = 0; i < n; i++) {   // зчитуємо елементи
            arr[i] = sc.nextInt();
        }

        for (int i = n - 1; i >= 0; i--) { // виводимо у зворотному порядку
            System.out.print(arr[i]);
            if (i > 0) System.out.print(" "); // додаємо пробіл між елементами
        }
    }
}
